        <category>general</category>
        <client_script><![CDATA[var app=angular.module("myApp2",[]);
app.controller("myCtrl",function($scope){
		
	$scope.agtName=g_user.userName;
	$scope.showBtnLogin=true;
	$scope.showBtnReady=true;
	$scope.showBtnPause=true;
	$scope.showBtnLogout=true;
	
	$scope.clickedLogin=function(){alert("Login");};
	$scope.clickedReady=function(){alert("Ready");};
	$scope.clickedPause=function(){alert("Pause");};
	$scope.clickedLogout=function(){alert("Logout");};

});]]></client_script>
