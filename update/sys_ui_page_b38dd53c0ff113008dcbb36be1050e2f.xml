		  <div class="w3-row w3-large w3-light-grey">
			<div class="w3-col s3">
			  <button ng-click="clickedLogin()" class="w3-button w3-ripple w3-block" ng-disabled="!showBtnLogin">Login</button>
			</div>
			<div class="w3-col s3">
				<button ng-click="clickedReady()" class="w3-button w3-ripple w3-block" ng-disabled="!showBtnReady">Ready</button>
