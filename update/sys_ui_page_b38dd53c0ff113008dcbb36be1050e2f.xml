				<button ng-click="clickedLogout()" class="w3-button w3-ripple w3-block" ng-disabled="!showBtnLogout">Logout</button>
			</div>
			  <div class="w3-col s3">
				  <span>{{agtName}}</span>
			</div>
		  </div>
		</div>
	</div>
